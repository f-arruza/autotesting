{{ container_label }}:
    build:
        context: ./dockerfiles/{{ container_desc }}
        dockerfile: Dockerfile
    container_name: {{ container_label }}
    depends_on:
        - hub{% for browser in browsers %}
        - {{ browser }}{% endfor %}
    environment:
        - HUB_PORT_4444_TCP_ADDR=hub
        - HUB_PORT_4444_TCP_PORT=4444
        - TEST_PLAN={{ plan_id }}
    volumes:
        - {{ source_path }}:/app
        - ./src/results/jsons:/reports
