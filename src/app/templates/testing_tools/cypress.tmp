{{ container_label }}:
    build:
        context: ./dockerfiles/{{ container_desc }}
        dockerfile: Dockerfile
    container_name: {{ container_label }}
    command: {{ command }}
    environment:
        - TEST_PLAN={{ plan_id }}
    volumes:
        - {{ source_path }}:/install/test
        - ./src/results/jsons:/reports
        - ./src/results:/media
